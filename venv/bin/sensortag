#!/home/skyler/Python-Project/ble-test/venv/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'bluepy==1.3.0','console_scripts','sensortag'
__requires__ = 'bluepy==1.3.0'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('bluepy==1.3.0', 'console_scripts', 'sensortag')()
    )
